{
    "name": "zicht/admin-bundle",
    "description": "Admin utilities for Symfony",
    "type": "symfony-bundle",
    "repositories": [],
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^8",
        "symfony/console": "^5.4 || ^6",
        "symfony/yaml": "^5.4 || ^6",
        "sonata-project/doctrine-orm-admin-bundle": "^4",
        "zicht/framework-extra-bundle": "^10 || ^11.x-dev",
        "twig/twig": "^2.13 || ^3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^9.1",
        "vimeo/psalm": "^5.16",
        "zicht/page-bundle": "^9.x-dev",
        "zicht/url-bundle": "^8.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\AdminBundle\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "scripts": {
        "analysis": "psalm --no-cache",
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-feature-symfony-6": "9.x-dev"
        }
    }
}
