{
    "name": "zicht/admin-bundle",
    "description": "Admin utilities for Symfony",
    "type": "symfony-bundle",
    "repositories": [],
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/console": "^4.4",
        "sonata-project/doctrine-orm-admin-bundle": "^4",
        "zicht/framework-extra-bundle": "^9",
        "sensio/framework-extra-bundle": "^5",
        "twig/twig": "^2.9 || ^3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\AdminBundle\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\AdminBundle\\": [
                "tests/"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "scripts": {
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "test": "phpunit tests/"
    }
}
